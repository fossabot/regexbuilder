{
  "name": "regexbuilder",
  "version": "0.0.0-development",
  "author": {
    "name": "Tilmann Meyer",
    "email": "allescrafterx@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NieLeben/regexbuilder"
  },
  "bugs": {
    "url": "https://github.com/NieLeben/regexbuilder/issues"
  },
  "main": "src/pages/index.js",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "now-build": "next build",
    "lint-staged": "lint-staged",
    "lint": "eslint --ignore-path .gitignore --fix --ext .js,.jsx ./",
    "storybook": "yarn run prebuild:storybook && start-storybook -p 6006",
    "prebuild:storybook": "yarn run test:generate-output",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | codacy-coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "rootDir": "./",
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "verbose": true
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-source-maps": "^0.0.3",
    "core-js": "3",
    "next": "8.0.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-jest": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "^1.3.1",
    "install-peers": "1.0.3",
    "jest": "^24.7.1",
    "lint-staged": "8.1.5",
    "preact": "8.4.2",
    "prettier": "1.16.4",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.6",
    "webpack": "4.29.6",
    "codacy-coverage": "^3.4.0"
  }
}
